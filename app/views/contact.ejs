<%- include ('partials/header.ejs') %>
<%- include ('partials/main_nav.ejs') %>
   

<main class="container contact" title="Contact">
  <div class="jumbotron">
    <h1 class="display-4">Let's Chat!</h1>
    <form action="/">
      <label for="firstName">First Name:</label>
      <input type="text" id="firstName" name="firstName" required><br><br>

      <label for="lastName">Last Name:</label>
      <input type="text" id="lastName" name="lastName" required><br><br>

      <label for="contactNumber">Contact Number:</label>
      <input type="tel" id="contactNumber" name="contactNumber" required><br><br>

      <label for="emailAddress">Email Address:</label>
      <input type="email" id="emailAddress" name="emailAddress" required><br><br>

      <label for="message">Message:</label><br>
      <textarea id="message" name="message" rows="4" required></textarea><br><br>

      <input type="submit" id="submitButton" value="Submit" style="background-color: rgb(27, 31, 35); color: #fff; padding: 10px 15px; border: none; border-radius: 5px; cursor: pointer;">
      
  </form>

  
    </div>
    </main>
    
